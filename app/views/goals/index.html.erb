<table class="table table-hover ">
  <h1>Completed Tasks</h1>
    <thead>
      <tr>
        <th>Title</th>
        <th>Created at</th>
        <th>Completed</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @goal.each do |goal| %>
        <tr>
           <td>
             <strong>
               <%= goal.title %>
             </strong>
           </td>
 
         
           <td class="text-info">
             <%= goal.created_at %>
           </td>
           <td class="text-success">
             <%= goal.due_date %>
           </td>
        </tr>
        <% end %>
     </tbody>
   </table>

   <%= link_to "New task", goals_new_path%>